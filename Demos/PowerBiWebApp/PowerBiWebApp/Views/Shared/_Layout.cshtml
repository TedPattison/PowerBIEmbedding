<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Power BI Web App</title>
  <link href="~/Content/bootstrap.css" rel="stylesheet" />
  <link href="~/Content/font-awesome.css" rel="stylesheet" />
  <link href="~/App/App.css" rel="stylesheet" />
</head>

<body>

  <div id="banner" class="container">
    <nav id="topnav" class="navbar navbar-expand-sm navbar-dark bg-dark">
      <ul class="navbar-nav">
        <li class="nav-item active">
          @Html.ActionLink("Power BI Web App", "Index", "Home",
                            routeValues: null, htmlAttributes: new { @class = "nav-link navbar-brand" })
        </li>
        <li class="nav-item active">
          @Html.ActionLink("Workspaces", "Workspaces", "Home",
                            routeValues: null, htmlAttributes: new { @class = "nav-link" })
        </li>
        <li class="nav-item active">
          @Html.ActionLink("Admin Workspaces", "AdminWorkspaces", "Home",
                            routeValues: null, htmlAttributes: new { @class = "nav-link" })
        </li>

        <li class="nav-item active">
          @Html.ActionLink("Client-side Embedding", "ClientSideEmbedding", "Home",
                            routeValues: null, htmlAttributes: new { @class = "nav-link" })
        </li>


      </ul>
      @Html.Partial("_LoginPartial")
    </nav>
  </div>

  <!-- Add Main Body Content Here -->
  <div id="content-box" class="container body-content">
    @RenderBody()
  </div>

  <script src="~/Scripts/jquery-3.5.0.js"></script>
  <script src="~/Scripts/umd/popper.min.js"></script>
  <script src="~/Scripts/bootstrap.js"></script>
  <script src="~/Scripts/powerbi.js"></script>
  <script src="~/App/app.js"></script>

</body>
</html>
